    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.13",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.27",
    "prettier-config-custom": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "tsconfig": "workspace:*",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
